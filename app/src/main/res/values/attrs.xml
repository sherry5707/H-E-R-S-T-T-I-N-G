<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_fill_color" format="color" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="CountDownTextView">
        <attr name="cdt_hint_text" format="string" />
        <attr name="cdt_countdown_time" format="integer" />
        <attr name="cdt_usable_color" format="color" />
        <attr name="cdt_unusable_color" format="color"/>
        <attr name="cdt_normal_text" format="string"/>
    </declare-styleable>
</resources>